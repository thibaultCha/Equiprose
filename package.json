{
  "name": "equiprose",
  "author": {
    "name": "Thibault Charbonnier",
    "email": "thibaultcha@me.com"
  },
  "version": "1.1.1",
  "description": "A static website and blog generator",
  "keywords": [
    "static",
    "blog",
    "generator",
    "website",
    "markdown"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "equiprose": "./bin/equiprose"
  },
  "config": {
    "blanket": {
      "data-cover-only": "lib",
      "data-cover-never": ["node_modules", "index", "serve"]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thibaultCha/Equiprose.git"
  },
  "dependencies": {
    "glob": "3.2.x",
    "express": "3.4.x",
    "commander": "2.1.x",
    "jade": "1.1.x",
    "stylus": "0.42.x",
    "marked": "0.3.x",
    "enumerate-properties": "1.0.x",
    "moment": "2.5.x",
    "emoji-images": "0.1.x",
    "colors": "0.6.x",
    "yamljs": "0.1.x",
    "fs-extra": "0.8.x",
    "pygmentize-bundled": "2.1.x",
    "node-watch": "^0.3.4"
  },
  "devDependencies": {
    "mocha": "1.17.x",
    "blanket": "1.1.x",
    "coveralls": "2.7.x",
    "mocha-lcov-reporter": "0.0.x"
  }
}
